pipeline {
    agent any
    stages {
        stage('Stage@1') {
            steps {
                git branch: 'main', url: 'https://github.com/DEVOPSDEVSECOPSKISHOR/MULTI_PIPELINE_01.git'
               }
        }
        stage('stage@2'){
            steps{
                sh '''
                     echo "$WORKSPACE"
                '''    
}    
    }
        stage('stage@3'){
            steps{
                script{
                    configFile="$WORKSPACE/config.properties"
                    echo configFile
                       
                }
            }
}
}
}
