def username
def password
def configFile
def config
pipeline {
    agent any

    stages {

stage('stage-checkout'){
    steps{
    cleanWs();
    git branch: 'main', url: 'https://github.com/DEVOPSDEVSECOPSKISHOR/MULTI_PIPELINE_01.git'
}
}
stage('condotion'){
steps{
script{
configFile="config.properties"
config = [:]
username=config['username']
passwd=config['password']
echo username
echo passwd
try{
config = readProperties file: configFile
}
catch (Exception e) {
echo "failed to load properties file ${e.message}"
error "Failed to laod properties file ${e.message}"


}
}
}
}
        stage('stage-01') {
            steps {
script{
                    env.USER=username
                    env.PASS=passwd
                sh '''
                    echo "credentials $USER"
                    echo "passeord is $PASS"
                '''
            }
            }
        }
    }
}
