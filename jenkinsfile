


pipeline {
    agent any

    stages {

stage('stage-checkout'){
    steps{
    cleanWS();
    git branch: 'main', url: 'https://github.com/DEVOPSDEVSECOPSKISHOR/MULTI_PIPELINE_01.git'
}
}
stage('condotion'){
steps{
script{
def configFile="config.properties"
def config = [:]
def user="username"
def passwd="password"
try{
config = readProperties file: configFile
}
catch (Exception e) {
echo "failed to load properties file ${e.message}"
error "Failed to laod properties file ${e.message}"


}
}
}
}
        stage('stage-01') {
            steps {
script{
                    env.USER=user
                sh '''
                    echo "credentials $USER"
                '''
            }
            }
        }
    }
}
